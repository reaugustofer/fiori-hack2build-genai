{
  "_version": "1.48.0",
  "sap.app": {
    "id": "numenit.com.hack2buildgenai",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:basic",
      "version": "1.8.4",
      "toolsId": "c26378ae-d434-4cec-8c63-676c9abef13f"
    },
    "dataSources": {
      "mainService": {
        "uri": "/sap/opu/odata/",
        "type": "OData",
        "settings": {
          "annotations": [],
          "localUri": "localService/metadata.xml",
          "odataVersion": "2.0"
        }
      },
      "newsService": {
        "type": "JSON",
        "uri": "model/MockNews.json"
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": true,
    "dependencies": {
      "minUI5Version": "1.120.0",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {},
        "sap.f": {},
        "sap.suite.ui.generic.template": {},
        "sap.ui.comp": {},
        "sap.ui.generic.app": {},
        "sap.ui.table": {},
        "sap.ushell": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "numenit.com.hack2buildgenai.i18n.i18n"
        }
      },
      "": {
        "dataSource": "mainService",
        "preload": true,
        "settings": {}
      },
      "newsJSONModel": {
        "type": "sap.ui.model.json.JSONModel",
        "dataSource": "newsService"
      },
      "newsModel": {
        "type": "sap.ui.model.json.JSONModel"
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "viewType": "XML",
        "async": true,
        "viewPath": "numenit.com.hack2buildgenai.view",
        "controlAggregation": "pages",
        "controlId": "app",
        "clearControlAggregation": false,
        "bypassed": {
          "target": "NotFound"
        }
      },
      "routes": [
        {
          "name": "RouteMain",
          "pattern": ":?query:",
          "target": [
            "TargetMain"
          ]
        },
        {
          "name": "RouteList",
          "pattern": "NewsList/RawMaterial='{RawMaterial}'&Region='{Region}'",
          "target": [
            "TargetList"
          ]
        }
      ],
      "targets": {
        "TargetMain": {
          "viewType": "XML",
          "transition": "slide",
          "viewId": "Main",
          "viewName": "Main",
          "viewLevel": 1
        },
        "TargetList": {
          "viewType": "XML",
          "transition": "slide",
          "viewId": "List",
          "viewName": "List",
          "viewLevel": 2
        },
        "NotFound": {
          "viewType": "XML",
          "transition": "show",
          "viewId": "NotFound",
          "viewName": "NotFound"
        }
      }
    },
    "rootView": {
      "viewName": "numenit.com.hack2buildgenai.view.App",
      "type": "XML",
      "async": true,
      "id": "App"
    }
  }
}